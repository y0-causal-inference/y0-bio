@article{bohmwald_2018,
title = {Neurologic alterations due to respiratory virus infections.},
author = {Bohmwald, Karen and GÃ¡lvez, NicolÃ¡s M S and RÃ­os, Mariana and Kalergis, Alexis M},
pages = {386},
url = {http://dx.doi.org/10.3389/fncel.2018.00386},
year = {2018},
