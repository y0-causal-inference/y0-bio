@article{fairbanks_2017,
title = {The {ENGases}: versatile biocatalysts for the production of homogeneous N-linked glycopeptides and glycoproteins.},
author = {Fairbanks, Antony J},
pages = {5128-5146},
url = {http://dx.doi.org/10.1039/c6cs00897f},
year = {2017},
month = {aug},
