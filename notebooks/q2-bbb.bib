@article{schoneboom_2000,
title = {Early expression of {IFN}-alpha/beta and {iNOS} in the brains of Venezuelan equine encephalitis virus-infected mice.},
author = {Schoneboom, B A and Lee, J S and Grieder, F B},
pages = {205-215},
url = {http://dx.doi.org/10.1089/107999000312621},
year = {2000},
month = {feb},
