@article{yun_2009,
title = {{CD4}+ T cells provide protection against acute lethal encephalitis caused by Venezuelan equine encephalitis virus.},
author = {Yun, Nadezhda E and Peng, Bi-Hung and Bertke, Andrea S and Borisevich, Viktoriya and Smith, Jennifer K and Smith, Jeanon N and Poussard, Allison L and Salazar, Milagros and Judy, Barbara M and Zacks, Michele A and Estes, D Mark and Paessler, Slobodan},
pages = {4064-4073},
url = {http://dx.doi.org/10.1016/j.vaccine.2009.04.015},
year = {2009},
month = {jun},
day = {19},
urldate = {2021-05-21},
journal = {Vaccine},
volume = {27},
number = {30},
doi = {10.1016/j.vaccine.2009.04.015},
pmid = {19446933},
pmcid = {PMC2741389},
sciwheel-projects = {{COVID}-19},
abstract = {Studying the mechanisms of host survival resulting from viral encephalitis is critical to the development of vaccines. Here we have shown in several independent studies that high dose treatment with neutralizing antibody prior to intranasal infection with Venezuelan equine encephalitis virus had an antiviral effect in the visceral organs and prolonged survival time of infected mice, even in the absence of alphabeta T cells. Nevertheless, antibody treatment did not prevent the development of lethal encephalitis. On the contrary, the adoptive transfer of primed {CD4}(+) T cells was necessary to prevent lethal encephalitis in mice lacking alphabeta T cell receptor.}
}
@article{rhea_2021,
title = {The S1 protein of {SARS}-{CoV}-2 crosses the blood-brain barrier in mice.},
author = {Rhea, {EM} and Logsdon, {AF} and Hansen, {KM} and Williams, {LM} and Reed, {MJ} and Baumann, {KK} and Holden, {SJ} and Raber, J and Banks, {WA} and Erickson, {MA}},
pages = {368-378},
url = {http://www.nature.com/articles/s41593-020-00771-8},
year = {2021},
month = {mar},
urldate = {2021-03-27},
journal = {Nature Neuroscience},
volume = {24},
number = {3},
issn = {1097-6256},
doi = {10.1038/s41593-020-00771-8},
pmid = {33328624},
sciwheel-projects = {{COVID}-19},
abstract = {It is unclear whether severe acute respiratory syndrome coronavirus 2, which causes coronavirus disease 2019, can enter the brain. Severe acute respiratory syndrome coronavirus 2 binds to cells via the S1 subunit of its spike protein. We show that intravenously injected radioiodinated S1 (I-S1) readily crossed the blood-brain barrier in male mice, was taken up by brain regions and entered the parenchymal brain space. I-S1 was also taken up by the lung, spleen, kidney and liver. Intranasally administered I-S1 also entered the brain, although at levels roughly ten times lower than after intravenous administration. {APOE} genotype and sex did not affect whole-brain I-S1 uptake but had variable effects on uptake by the olfactory bulb, liver, spleen and kidney. I-S1 uptake in the hippocampus and olfactory bulb was reduced by lipopolysaccharide-induced inflammation. Mechanistic studies indicated that I-S1 crosses the blood-brain barrier by adsorptive transcytosis and that murine angiotensin-converting enzyme 2 is involved in brain and lung uptake, but not in kidney, liver or spleen uptake.}
}
