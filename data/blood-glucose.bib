@article{li_2020b,
title = {Newly diagnosed diabetes is associated with a higher risk of mortality than known diabetes in hospitalized patients with {COVID}-19.},
author = {Li, Huiqing and Tian, Shenghua and Chen, Ting and Cui, Zhenhai and Shi, Ningjie and Zhong, Xueyu and Qiu, Kangli and Zhang, Jiaoyue and Zeng, Tianshu and Chen, Lulu and Zheng, Juan},
