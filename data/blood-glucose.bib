@article{corstjens_2006,
title = {Hyperglycaemia in critically ill patients: marker or mediator of mortality?},
author = {Corstjens, Anouk M and van der Horst, Iwan C C and Zijlstra, Jan G and Groeneveld, A B Johan and Zijlstra, Felix and Tulleken, Jaap E and Ligtenberg, Jack J M},
pages = {216},
